{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "off",
    "@typescript-eslint/no-explicit-any": "off",  // Mengubah error menjadi peringatan
    "react-hooks/exhaustive-deps": "off",         // Menjadikannya peringatan agar tetap mendapat notifikasi
    "prefer-const": "off"                          // Menonaktifkan prefer-const
  }
}
